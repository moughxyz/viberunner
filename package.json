{
  "name": "viberunner-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@4.9.2",
  "scripts": {
    "install": "yarn install",
    "clean": "yarn workspaces foreach run clean",
    "build": "yarn workspaces foreach run build",
    "dev": "yarn workspaces foreach --parallel run dev"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}

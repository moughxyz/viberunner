{
  "name": "viberunner",
  "version": "0.0.16",
  "description": "Vibe app desktop manager.",
  "main": "dist-electron/index.js",
  "homepage": "https://github.com/moughxyz/viberunner-releases",
  "repository": {
    "type": "git",
    "url": "https://github.com/moughxyz/viberunner-releases.git"
  },
  "author": "Viberunner Team",
  "license": "MIT",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:debug": "DEBUG=electron-forge:* npm run make",
    "rebuild": "npm rebuild && npx @electron/rebuild",
    "publish": "electron-forge publish",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:apps": "cd ExampleApps && for dir in */; do if [ -f \"$dir/package.json\" ]; then echo \"Building $dir...\" && cd \"$dir\" && npm run build && cd ..; fi; done",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview",
    "convert-icon": "node scripts/convert-icon.js assets/source-icon.png",
    "release": "node scripts/release.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@electron/remote": "^2.1.2",
    "@radix-ui/react-compose-refs": "^1.1.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@sentry/react": "^9.27.0",
    "@tailwindcss/vite": "^4.1.8",
    "@viberunner/common": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-log": "^5.4.0",
    "fix-path": "^4.0.0",
    "lucide-react": "^0.511.0",
    "mime-types": "^2.1.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "strip-ansi": "^7.1.0",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.3.0",
    "@electron-forge/maker-deb": "^7.3.0",
    "@electron-forge/maker-dmg": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.3.0",
    "@electron-forge/maker-squirrel": "^7.3.0",
    "@electron-forge/maker-zip": "^7.3.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.3.0",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.3.0",
    "@electron-forge/publisher-github": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^3.6.2",
    "@electron/universal": "^2.0.3",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/typography": "^0.5.16",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.5.0",
    "electron": "^29.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.5.4",
    "postcss-loader": "^8.1.1",
    "tailwindcss": "^4.1.8",
    "tw-animate-css": "^1.3.3",
    "typescript": "^5.2.2",
    "update-electron-app": "^3.1.1",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.0"
  }
}

{
  "name": "@viberunner/common",
  "version": "1.0.0",
  "description": "Common types and static values for Viberunner projects",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && cp src/*.json dist/ 2>/dev/null || true",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.2.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./product": {
      "types": "./dist/product.d.ts",
      "import": "./dist/product.js"
    },
    "./release.json": "./dist/release.json"
  }
}
